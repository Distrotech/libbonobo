#include <Bonobo_Unknown.idl>

module Bonobo {
	typedef sequence<string> StringList;
        
        exception GeneralError { 
                string description; 
        };

	interface GenericFactory : Bonobo::Unknown {
		exception CannotActivate { };

		Object createObject (in string oaf_id)
